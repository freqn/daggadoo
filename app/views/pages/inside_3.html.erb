<div class="center_devise">
    <% if user_signed_in? %>
    <div class="media-element">
      <%= image_tag('http://www.scified.com/pics/75.gif', class: 'cat-gif') %>
    </div>
    <%= link_to "Proceed?", root_path, class: "btn btn-success btn-lg btn-gif"%>
    <% else %>
    <h1>Welcome to SpacedOut</h1>
    <%= link_to "Sign up", new_user_registration_path, class: "btn btn-success btn-lg"%>
    <%= link_to "Log in", user_session_path, class: "btn btn-default btn-lg"%>
    <h3>Register and/or log in to see our features</h3>
    <% end %>
</div>
